<div class="wrapper top fadeInDown">
<div id="formContent" class="large">
  <div class="container">
    <div class="row">
        <div class="col s12">
          <ul class="tabs">
            <% for (var i = 0; i < cantDias; i++) { %>
                <li class="tab col s3"><a <% if(i==0){ %> class="active" <% } %> href="#test<%= i %>">DÃ­a <%= i+1 %></a></li>
            <% } %>
          </ul>
        </div>
      </div>
      <div class="row">
      <% for (var i = 0; i < cantDias; i++) { %>
          <div id="test<%= i %>" class="col s12">
              <% _.each(dirMomentos[i], function(momento) { %>
                      <%= momento.nombre %>
                      <%= momento.horaInicio %>
                      <%= momento.horaFin %>
                      <small><%= momento.diaInicio %></small>

              <% }) %>
          </div>
      <% } %>
      </div>
  </div>
</div>
</div>
